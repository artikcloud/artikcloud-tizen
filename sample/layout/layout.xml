					align_h="fill" pack_w="1000" pack_x="0" pack_y="0" weight_v="1">

					<label wrap_width="0"
						text="&lt;align=center&gt;&lt;font_size=80&gt;&lt;backing_color=FFFF00&gt;Artik Cloud&lt;/backing_color&gt;&lt;/font&gt;&lt;/align&gt;"
						visible="true" align_v="fill" ellipsis="false" weight_h="1" id="label4"
						line_wrap="none" align_h="fill" disable="false" weight_v="1">
					</label>
				</box>

			</grid>
		</view>
	</views>
</document>
