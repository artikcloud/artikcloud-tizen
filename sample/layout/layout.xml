				</button>

				<box homogeneous="false" padding_v="0" visible="true" align_v="fill"
					direction="vertical" pack_h="500" weight_h="1" id="box1" padding_h="0"
					align_h="fill" pack_w="500" pack_x="100" pack_y="150" weight_v="1">			
				</box>
				
				<box homogeneous="false" padding_v="0" visible="true" align_v="fill"
